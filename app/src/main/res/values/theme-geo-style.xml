    <style name="GeoStyleTheme" parent="AppTheme">
        <item name="android:textColor">@android:color/black</item>
        <item name="android:textColorHint">@android:color/black</item>
        <item name="android:windowBackground">@drawable/bg_geo</item>
        <item name="android:buttonStyle">@style/GeoButtonStyle</item>
        <item name="android:imageButtonStyle">@style/GeoButtonStyle</item>
        <item name="sepBg">@color/geo_sep</item>
        <item name="iconColor">@color/geo_sep</item>

        <item name="itemBgMe">@drawable/geo_bg_me</item>
